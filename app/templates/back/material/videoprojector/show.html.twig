{% extends 'base.html.twig' %}

{% block title %}Videoprojector{% endblock %}

{% block body %}

<div class="index__container"> 

    <div class="index__container-title">
        <h1>Videoprojecteur</h1>
    </div>

    <div class="show__container-table">

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ videoprojector.id }}</td>
            </tr>
            <tr>
                <th>Model</th>
                <td>{{ videoprojector.model }}</td>
            </tr>
            <tr>
                <th>PurchaseDate</th>
                <td>{{ videoprojector.purchaseDate ? videoprojector.purchaseDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ videoprojector.status }}</td>
            </tr>
        </tbody>
    </table>

    <div class="show__container-btn">

    <a class="btn-list" href="{{ path('app_back_material_videoprojector_index') }}">back to list</a>

    <a class="btn-update" href="{{ path('app_back_material_videoprojector_edit', {'id': videoprojector.id}) }}">edit</a>

    {{ include('back/material/videoprojector/_delete_form.html.twig') }}

    </div>

    </div>

</div>
{% endblock %}
